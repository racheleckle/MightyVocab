<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="314.0" prefWidth="306.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ManageSetsCodeBehind">
   <children>
      <AnchorPane prefHeight="720.0" prefWidth="960.0" style="-fx-background-color: oldlace;">
         <children>
            <Label fx:id="setManagerLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="324.0" layoutY="45.0" text="Set Manager" textFill="SLATEGRAY">
               <font>
                  <Font name="Harlow Solid Italic" size="64.0" />
               </font>
            </Label>
            <Label fx:id="setLabel" layoutX="297.0" layoutY="213.0" text="Set:" textFill="DARKSLATEGRAY">
               <font>
                  <Font name="Cambria" size="24.0" />
               </font>
            </Label>
            <Label fx:id="invalidCredentialsLabel" layoutX="280.0" layoutY="264.0" text="Incorrect username or password. Please try again." textFill="#c00000" visible="false">
               <font>
                  <Font name="Cambria" size="18.0" />
               </font>
            </Label>
            <Label fx:id="cardsLabel" layoutX="297.0" layoutY="275.0" text="Cards:" textFill="DARKSLATEGRAY">
               <font>
                  <Font name="Cambria" size="18.0" />
               </font>
            </Label>
            <TextField fx:id="setNameTextField" layoutX="351.0" layoutY="210.0" prefHeight="37.0" prefWidth="343.0" style="-fx-border-color: slategrey;">
               <font>
                  <Font name="Cambria" size="18.0" />
               </font>
            </TextField>
            <Button fx:id="addCardsButton" layoutX="360.0" layoutY="268.0" mnemonicParsing="false" onAction="#addCards" text="+">
               <font>
                  <Font name="Cambria" size="18.0" />
               </font>
            </Button>
            <Button fx:id="removeCardsButton" layoutX="404.0" layoutY="268.0" mnemonicParsing="false" onAction="#removeCards" prefHeight="35.0" prefWidth="34.0" text="-">
               <font>
                  <Font name="Cambria" size="18.0" />
               </font>
            </Button>
            <ListView fx:id="cardsListView" layoutX="297.0" layoutY="310.0" prefHeight="267.0" prefWidth="397.0" />
            <Button fx:id="previousButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#previous" prefHeight="35.0" prefWidth="125.0" text="&lt;--- Previous">
               <font>
                  <Font name="Cambria" size="18.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
